    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/plugins/isotope/jquery.fancybox.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/plugins/isotope/isotope.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/plugins/slick/slick.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/plugins/slick/slick-theme.css")">
